{
  "name": "zonely.eu",
  "version": "1.0.0",
  "description": "World time zone converter",
  "main": "index.js",
  "homepage": "https://sebastianrosik.github.io/zonely/",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "develop": "webpack-dev-server --open --mode development",
    "start": "npm run develop",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build",
    "release": "standard-version"
  },
  "author": "Sebastian Rosik <sebastian.rosik@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@welldone-software/why-did-you-render": "^2.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-shortcuts": "^2.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "moment-timezone": "^0.5.23",
    "standard-version": "^4.4.0"
  },
  "jest": {
    "setupFiles": [
      "./setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
